<table class="table table-striped">
	<thead>
		<tr>
			<th>ID</th>
			<th>Name</th>
			<th>Phone</th>
			<th>Date and Time</th>
			<th>Barber</th>
			<th>Color</th>
		</tr>
	
	</thead>	

	<tbody>
	<% @results.each do |row| %>
	
	<tr>
		<td><%= row['ID'] %></td>
		<td><%= row['Name'] %></td>
		<td><%= row['Phone'] %></td>
		<td><%= row['DateStamp'] %></td>
		<td><%= row['Barber'] %></td>
		<td><%= row['Color'] %></td>
	</tr>
	
	<% end %>
	</tbody>
</table>